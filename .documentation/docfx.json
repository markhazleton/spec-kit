{
  "build": {
    "content": [
      {
        "files": [
          "*.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": [
          "media/**"
        ],
        "src": "..",
        "dest": "media"
      }
    ],
    "overwrite": [],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Spec Kit Spark",
      "_appName": "Spec Kit Spark",
      "_appFooter": "Spec Kit Spark - Constitution-powered specification-driven development toolkit",
      "_enableSearch": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/MarkHazleton/spec-kit",
        "branch": "main"
      }
    }
  }
}

